            ],
            "legend": {
                "align": "right",
                "verticalAlign": "middle",
                "itemStyle": {
                    "color": "#34495e"
                }
            },
            "plotOptions": {
                "bellcurve": {
                    "events": {
                        "afterRender": "function() { this.points.forEach((p,i) => { p.dataLabel = ['-3\u03c3','-2\u03c3','-\u03c3','\u03bc','+\u03c3','+2\u03c3','+3\u03c3'][i] })}"
                    }
                }
            }
        }
