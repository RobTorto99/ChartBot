{
    "visualization_prompt": "Generate a column chart with bellcurve overlay showing synthetic sales performance data distribution using \u03bc=50 and \u03c3=15 normal distribution simulation",
    "visualization_code": {
        "chartOptions": {
            "chart": {
                "type": "column",
                "margin": [
                    60,
                    120,
                    60,
                    60
                ],
                "backgroundColor": "#f9f9f9"
            },
            "title": {
                "text": "Sales Performance Distribution Analysis",
                "style": {
                    "color": "#2c3e50",
                    "fontSize": "20px",
                    "fontWeight": "bold"
                }
            },
            "xAxis": [
                {
                    "title": {
                        "text": "Performance Score",
                        "style": {
                            "color": "#2980b9",
                            "fontSize": "14px"
                        }
                    },
                    "visible": true,
                    "gridLineColor": "#ecf0f1"
                },
                {
                    "title": {
                        "text": "Normal Distribution"
                    },
                    "opposite": true,
                    "visible": false
                }
            ],
            "yAxis": [
                {
                    "title": {
                        "text": "Frequency",
                        "style": {
                            "color": "#27ae60",
                            "fontSize": "14px"
                        }
                    },
                    "gridLineColor": "#ecf0f1"
                },
                {
                    "title": {
                        "text": "Probability Density"
                    },
                    "opposite": true,
                    "visible": false
                }
            ],
            "series": [
                {
                    "name": "Performance Distribution",
                    "type": "column",
                    "data": [
                        12,
                        18,
                        27,
                        35,
                        47,
                        53,
                        49,
                        41,
                        32,
                        21,
                        15,
                        9
                    ],
                    "color": "#2ecc71",
                    "borderColor": "#27ae60"
                },
                {
                    "name": "Normal Distribution",
                    "type": "bellcurve",
                    "baseSeries": 0,
                    "intervals": 3,
                    "pointsInInterval": 7,
                    "color": "#e74c3c",
                    "marker": {
                        "enabled": true,
                        "symbol": "diamond",
                        "radius": 5
                    },
                    "dataLabels": {
                        "enabled": true,
