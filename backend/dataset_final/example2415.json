{
    "visualization_prompt": "Generate a network graph showing circular node connections using Highcharts' force-directed layout algorithm",
    "visualization_code": {
        "chart": {
            "type": "networkgraph",
            "backgroundColor": "#ffffff",
            "margin": 50
        },
        "title": {
            "text": "Network Graph Example",
            "style": {
                "fontSize": "16px",
                "color": "#444444"
            }
        },
        "plotOptions": {
            "networkgraph": {
                "layoutAlgorithm": {
                    "enableSimulation": true,
                    "friction": -0.9,
                    "integration": "verlet"
                },
                "dataLabels": {
                    "enabled": true,
                    "linkFormat": "{point.fromNode.name} \u2192 {point.toNode.name",
                    "format": "Node: {point.name}",
                    "style": {
                        "fontSize": "12px",
                        "color": "#333333"
                    }
                },
                "marker": {
                    "radius": 35
                }
            }
        },
        "series": [
            {
                "data": [
                    {
                        "id": "n1",
                        "name": "Node 1"
                    },
                    {
                        "id": "n2",
                        "name": "Node 2"
                    },
                    {
                        "id": "n3",
                        "name": "Node 3"
                    },
                    {
                        "id": "n4",
                        "name": "Node 4"
                    },
                    {
                        "id": "n5",
                        "name": "Node 5"
                    },
                    {
                        "from": "n1",
                        "to": "n2"
                    },
                    {
                        "from": "n2",
                        "to": "n3"
                    },
                    {
                        "from": "n3",
                        "to": "n4"
                    },
                    {
                        "from": "n4",
                        "to": "n5"
                    },
                    {
                        "from": "n5",
                        "to": "n1"
                    }
                ],
                "nodes": [
                    "n1",
                    "n2",
                    "n3",
                    "n4",
                    "n5"
